{
  "manifest_version": 3,
  "name": "SaveUp!",
  "version": "0.0.0.1",
  "description": "Displays compound interest potential of purchases",
  "icons": {
    "16": "/icons/icon16.png",
    "32": "/icons/icon32.png",
    "48": "/icons/icon48.png",
    "128": "/icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["/content/content.js"],
      "css": ["/content/styles.css"]
    }
  ],
  "permissions": ["storage"],
  "options_page": "/options/options.html"
}